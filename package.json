{
  "name": "@enriqcg/nestjs-amqp",
  "version": "0.3.0",
  "description": "AMQP module for NestJS with decorator support",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\"",
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc index.ts -d",
    "build:dev": "tsc index.ts -d && tsc -w -p tsconfig.build.json",
    "format": "prettier \"lib/**/*.ts\" --write",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnriqCG/nestjs-amqp.git"
  },
  "keywords": [
    "amqp",
    "amqplib",
    "rabbitmq-nestjs",
    "nestjs",
    "amqp-nestjs",
    "amqplib-nestjs"
  ],
  "author": "Enrique Carpintero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EnriqCG/nestjs-amqp/issues"
  },
  "homepage": "https://github.com/EnriqCG/nestjs-amqp#readme",
  "devDependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@types/amqplib": "^0.5.17",
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "amqplib": "^0.6.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0",
    "amqplib": "^0.6.0",
    "@types/amqplib": "^0.5.17"
  }
}
